pipeline {
    agent any
    stages {
        stage('build'){
            steps {
                script {
                    app = docker.build("python-app")
                }
            }
        }
        stage('test') {
            steps {
                echo 'Empty'
            }
        }
        stage('deploy') {
            steps {
                script {
                    docker.withRegistry('url del repositorio', 'credenciales')
                }
            }
        }
    }
}